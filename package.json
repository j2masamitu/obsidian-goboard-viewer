{
  "name": "goboard-viewer",
  "version": "1.1.6",
  "description": "Obsidian plugin to display SGF files as interactive Go boards with playback controls, variations, and markers",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "go",
    "sgf",
    "weiqi",
    "baduk",
    "sabaki",
    "goban",
    "variations",
    "markers"
  ],
  "author": "j2masamitu",
  "repository": {
    "type": "git",
    "url": "https://github.com/j2masamitu/obsidian-goboard-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/j2masamitu/obsidian-goboard-viewer/issues"
  },
  "homepage": "https://github.com/j2masamitu/obsidian-goboard-viewer#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@sabaki/shudan": "^1.7.1",
    "@sabaki/sgf": "^3.4.5",
    "preact": "^10.19.3"
  }
}
